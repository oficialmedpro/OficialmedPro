{
  "name": "sync-apis-oficialmed",
  "version": "1.0.0",
  "description": "APIs de sincronização para leads e oportunidades - OficialMed",
  "main": "api-sync-leads.js",
  "scripts": {
    "start:leads": "node api-sync-leads.js",
    "start:opportunities": "node api-sync-opportunities.js",
